@{
    Layout = "_LayoutAdmin";
}

<div class="text-left">
    <div class="text-black">
        <h2>List Account User</h2>
    </div>
</div>

<div class="card shadow mb-4">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="tableMember" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>IdUser</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>BirthDate</th>
                        <th>Gender</th>
                        <th>Phone</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody class="tbody"></tbody>
            </table>
        </div>
    </div>
</div>